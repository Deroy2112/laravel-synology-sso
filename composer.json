{
    "name": "deroy2112/laravel-synology-sso",
    "description": "Laravel Socialite driver for Synology SSO (OIDC) with PKCE S256, ID token verification, and group-to-role mapping",
    "keywords": [
        "laravel",
        "socialite",
        "synology",
        "sso",
        "oidc",
        "pkce",
        "authentication"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Deroy2112",
            "email": "deroy2112@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "laravel/framework": "^11.0|^12.0",
        "laravel/socialite": "^5.0",
        "firebase/php-jwt": "^6.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Deroy2112\\LaravelSynologySso\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deroy2112\\LaravelSynologySso\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deroy2112\\LaravelSynologySso\\SynologySsoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
